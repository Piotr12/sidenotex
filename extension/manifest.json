{
  "manifest_version": 3,
  "name": "Sidenotex",
  "version": "1.0",
  "description": "A tool for taking side notes on webpages.",
  "permissions": ["activeTab", "storage"],
  "host_permissions": ["https://sidenotex.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icons/icon.png"
    }
  }
}